programa {
  funcao inicio() {
    
    //Declaração
    real valor, quantidade, total

    //Entrada
    escreva("Qual o valor: ")
    leia (valor)
    escreva("Qual a quantidade: ")
    leia (quantidade)

    //Processamento
      total = valor * quantidade

      //Saída
      escreva("O preço final é: ", total)
  }
}
