programa {
  funcao inicio() {
    //Area de um retangulo
    //Declaração de variaveis
    inteiro x1,x2
    real area 

    //Entrada
    escreva("Qual é a base:")
    leia(x1)
    escreva("Qual é a altura:")
     leia(x2)
